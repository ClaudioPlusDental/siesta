<?xml version="1.0" encoding="UTF-8"?>
<entityList>

    <entity name="SongEntity" namespace="siestaphp\tests\functional\manager\gen\entity\ns2" table="ARTIST" targetPath="gen/entity/ns2">

        <construct name="Song" namespace="siestaphp\tests\functional\entity\ns2"/>
